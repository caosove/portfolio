.frm_top_line{border-top:2px solid #bbb;}
.tbl_frm01 .btn_frmline{border-radius:5px; margin-top:1.5px; padding:7px 10px; line-height:12px; font-size:12px; height:auto; font-family:'Nanum Gothic', serif; right 10px no-repeat; background-color:#5b5b5b;}
.tbl_frm01 .btn_frmline #autosave_count{color:#fff;}
.tbl_frm01 .frm_radio_m label{margin-right:8px;}
.tbl_frm01 #ad_default{margin-left:10px;}

