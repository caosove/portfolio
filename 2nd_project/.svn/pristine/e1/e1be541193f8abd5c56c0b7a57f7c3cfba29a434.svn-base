/* productList */
/* 상품리스트 */
#mainPadding {
	background-color: #FFF;
    width: 1060px;
    margin: 0 auto;
    min-height: auto;
}
#mainPadding .gridRow {
	max-width: 1060px;
    margin: 0 0 20px 0;
    border: 0;
    color: #333;
    width: 100%;
}
.productLists .gridRow {
	height: auto
}
.gridRow .categoryGray {
    padding-bottom: 2px;
    font-size: 12px;
    letter-spacing: 2px;
    text-transform: uppercase;
    color: #333;
    font-weight: normal;
}
.gridRow .categoryGray h2 {
    font-size: 12px;
    font-weight: normal;
    letter-spacing: 2px;
    margin: 0;
}
.gridRow .headOneGray {
    font-weight: bold;
    font-size: 18px;
    line-height: 20px;
    height: auto!important;
    letter-spacing: -1px;
    margin: 0 0 13px 0;
}
.gridRow .bodyTextGray {
    font-size: 12px;
    line-height: 18px;
    letter-spacing: 0;
    margin: 0 0 11px 0;
}
.gridRow .threeColumn {
	width:23.5%;	
    display: inline;
    float: left;
    border: 0;
    padding: 0;
    margin-right: 2%;
}
.productLists .product {
    position: relative;
    z-index: 0;
    margin-bottom: 60px;
}
.gridRow .threeColumn:nth-child(4n) {
    margin-right: 0;
}
.productLists .product .image {
    display: block;
    width: 100%;
    max-height: 250px;
    overflow: hidden;
    vertical-align: middle;
}
.product .image img {
    float: left;
}
.productLists .product .productDetails {
    display: block;
    height: auto;
    z-index: 1;
    background: #fff;
}
.productLists .product .productDetails .productTitle {
    display: block;
    float: none;    
    text-transform: uppercase;
    text-decoration: none;
    font-weight: bold;
    font-size: 14px;
    color: #333;    
    margin-bottom: 5px;
    padding-right: 5px;
    padding-top: 1px;
    cursor: pointer;
    font-style: normal;
}
.productLists .product .productDetails .productDesp {
    display: block;
    font-size: 12px;
    margin-bottom: 10px;    
    color: #666;
    line-height: 16px;
    text-decoration: none;
    padding-left: 2px;
    letter-spacing: -0.5px;
    font-style: normal;
}
.productLists .product .productDetails .price {
    display: block;
    position: relative;
    font-size: 26px;
    font-weight: bold;
    vertical-align: top;    
    text-decoration: none;
    line-height: 20px;
    color: #333;
    clear: left;
    font-style: normal;
}
.productLists .product .productDetails .price .unit {
    display: inline;
    font-size: 14px!important;
}
.productLists .product .productDetails .button-wrap {
	display: none;
	padding: 15px 5%;
	overflow: auto
}
.productLists .product .productDetails .button-wrap button {
	float: left;
	width: 49%;
	font-size: 13px;
	color: #fff;
	padding: 7px 0 10px;
	border: 0;
	outline: 0;	
	background: #0051ba;
}
.productLists .product .productDetails .button-wrap button:first-child {
	margin-right: 2%;
}
.productLists .product .productDetails .button-wrap button:hover {
	background: #327cc5
}
.productLists .product .productDetails:hover {	
	box-shadow: 5px 5px 10px rgba(0,0,0,.5);
	position: absolute;
	top: 0;
	left: 0;
	z-index: 100;
}
.productLists .product .productDetails:hover a {
	display: block;
	width: 100%;
	height: 100%;
	padding: 5% 5% 0;
}
.productLists .product .productDetails:hover .image > img {
	max-width: 100%
}
.productLists .product .productDetails:hover .button-wrap {
	display: block;	
}

/* 상품 등록 */
.product-regist-container {
	padding: 50px 0;
}
.product-regist {
	width: 90%;
	margin: 0 auto;
}
.product-regist th, .product-regist td { 
	padding: 5px 30px 5px 0; 
	font-size: 14px;
}
.product-regist th {
	text-align: right;
}
.product-regist input[type="text"],
.product-regist textarea {
	width: 100%;
	padding: 5px;
	font-size: 14px;
	color: #555;
}
.product-regist-container .button-wrap {
	padding: 50px 0;
	text-align: center;
}
.product-regist-container .button-wrap button {
	width: 200px;
	font-size: 16px;
	color: #fff;
	margin: 0 10px;	
	padding: 12px 0;
	border: 0;
	outline: 0;
}
.product-regist-container .button-wrap button:first-child {
	background: #898989;	
}
.product-regist-container .button-wrap button:last-child {
	background: #0051ba;	
}

