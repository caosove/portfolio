	color:#333;
	font-weight: bold;
}
.ikea-new-ui-body .feature-product-container .product-list > li .product-price {
	font-size:18px;
	vertical-align: top;
}
.ikea-new-ui-body .feature-product-container .product-list > li .product-price .currency-mark {
	font-size:14px;
	font-weight: normal;
	vertical-align: top;
}
.ikea-new-ui-body .feature-product-container .product-list > li .product-desc {
	margin:2px 0 5px;
	font-weight: normal;
	text-overflow: ellipsis;
	overflow: hidden;
	white-space: nowrap;
}

