.rightInfoDiv h1 {
    padding: 0;
    margin: 0;
    color: #000;
}
.rightInfoDiv h1 span.productName {
    font-size: 20px;
    line-height: 30px;
    letter-spacing: -1px;
    display: block;
}
.rightInfoDiv h1 span.productType {
    font-size: 13px;
    line-height: 18px;
    font-weight: normal;
    margin-bottom: .1em;
    display: block;
}
.rightInfoDiv .priceContainer {
    font-weight: bold;
    color: #000;
    font-size: 11px;
    padding-bottom: 6px;
    margin-bottom: 20px;
}
.rightInfoDiv .packagePrice {
    font-size: 38px;
    margin-right: 17px;
    color: #000;
    letter-spacing: -1.5px;
}
.rightInfoDiv .productunit {
	margin-right: 17px;
    font-size: 20px!important;
    font-weight: bold;
    letter-spacing: -0.5px;
    color: #000;
}
#rightNavContainer .salesArguments {
    font-size: 12px;
    line-height: 18px;
    color: #333;
    padding: 0;
    margin-bottom: 30px;
}
#rightNavContainer .rightNavSubContainer {
	margin-top: 15px
}
#rightNavContainer .quantityLabel {
    color: #666;
    font-size: 14px;
    line-height: 46px;
    padding-left: 15px;
}
#rightNavContainer #quantity {    
    float: right;
    width: 100px;
    height: 45px;
    padding: 0 18px;
    text-align: right;
    box-sizing: border-box;
    font-size: 12px;
    font-weight: bold;
    color: #444 !important;
    border-radius: 0;
    transition: all .3s;
    border: 1px solid #999;
}
.buttonDiv3 {
    display: block;
    margin: 10px 0;
}
#rightNavContainer .buttonContainer {
    position: inherit;
    top: -11px;
    left: -1px;
}
#rightNavContainer button.blueBtn, #rightNavContainer button.blueBtn:hover {
    position: relative;
    padding: 0;
    padding-left: 50px;
    width: 100%;
    height: 45px;
    line-height: 45px;
    vertical-align: top;
    border-radius: 0;
    outline: none;
    box-shadow: none;
    color: #fff;
    font-size: 16px;
    font-weight: bold;
    background: #155DA6;
    transition: all .3s;
}
#rightNavContainer button.blueBtn:before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    width: 50px;
    background: #124D8A url("../images/ico_cart_bbtn.png") no-repeat center center /45px auto;
}
#rightNavContainer button.blueBtn:hover {
	background: #327cc5;
}
#rightNavContainer #addCartContainer .buttonMar {
    position: relative;
    float: none;
    display: block;
    width: auto;
    padding: 0 !important;
    transition: all .3s;
    vertical-align: top;
}
#rightNavContainer #addCartContainer .buttonMar button {
    position: relative;
    padding: 0;
    padding-left: 50px;
    width: 180px;
    height: 45px;
    line-height: 45px;
    border-radius: 0;
    background: #D4D4D4;
    font-size: 12px;
    color: #656565;
}
#rightNavContainer #addCartContainer .buttonMar button:before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    width: 50px;
    background: #B3B3B3 url("../images/ico_cart_gbtn.png") no-repeat center center;
}
#rightNavContainer #addCartContainer .buttonMar button:hover {
	color:#7e7e7e;
	background:#e7e7e7;
}
#total-list-price {
    display: block;
    position: absolute;
    top: 0px;
    right: 0;
    line-height: 45px;
    font-size: 30px;
    font-weight: bold;
}
.stockChkTxtBold {
	padding: 20px 0 2px 0;
    margin: 30px 0;
    font-size: 12px;	
    border-top: 1px solid #ccc;
}