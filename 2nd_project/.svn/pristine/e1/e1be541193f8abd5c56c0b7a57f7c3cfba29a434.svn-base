/* feature products */
.ikea-new-ui-body .feature-product-container {
	position: relative;
	padding:30px 0;
}
.ikea-new-ui-body .feature-product-container .section-title {
	font-size:34px;
	text-align: center;
}
.ikea-new-ui-body .feature-product-container .section-subtitle {
	font-size:18px;
	font-weight: normal;
	text-align: center;
	margin-bottom: 23px;	
	margin-top:15px;
}
.ikea-new-ui-body .feature-product-container .product-list-container {
	position: relative;
	padding-bottom:50px;
	overflow: hidden;
}
.ikea-new-ui-body .feature-product-container .product-list > li {
	position: relative;
	float: left;
	width:16.666%;
	height:212px;
	padding-top:30px;
}
.ikea-new-ui-body .feature-product-container .product-list > li a {
	text-decoration: none;
}
.ikea-new-ui-body .feature-product-container .product-list > li > article {
	padding:0 30px;
}
.ikea-new-ui-body .feature-product-container .product-list > li .product-img {
	width:100%;
	margin-bottom: 15px;
}
.ikea-new-ui-body .feature-product-container .product-list > li .badge {
	display: block;	
	position:absolute;
	top:0px; left:30px;		
	color:#fff;
	font-size:13px;		
	width:45px;
	height:45px;
	border-radius: 45px;
	line-height: 45px;	
	text-align: center;	
	box-sizing: border-box;	
	text-transform: capitalize;	
}
.ikea-new-ui-body .feature-product-container .product-list > li .badge.new {
	background:#1294A3;
}
.ikea-new-ui-body .feature-product-container .product-list > li .product-name,
.ikea-new-ui-body .feature-product-container .product-list > li .product-desc,
.ikea-new-ui-body .feature-product-container .product-list > li .product-price {
	display: block;
