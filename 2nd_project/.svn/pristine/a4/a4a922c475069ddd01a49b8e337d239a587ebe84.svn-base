					<div class="sort-container">
						<form id="new-filter-form">
							<select class="sorting" name="sorting" onchange="changeSorting(this)">
								<option value="relevance">관련도순</option>
								<option value="name">이름순</option>
								<option value="price">가격순 </option>
								<option value="new">신규상품순</option>
							</select>
						</form>
						<script>
							function changeSorting(t) {
								
								$J('#new-filter-form').submit();
								
								return false;
								
								
								var v = $J(t).val();
								var l = location.toString();
								var patt = new RegExp("sorting");
								
								if ( patt.test(location.search) ) {
									var rPatt = new RegExp("sorting=[^\&]*");
									l = l.replace(rPatt, 'sorting=' + v);
									
									console.log(l);
								}
								else if ( location.search == '' ) {
									l = l + '?sorting=' + v;
								} else {
									l = l + '&sorting=' + v;
								}
								
								location.href = l;
							}
							// 필터 항목 세팅
							setFilterVal();
			
						</script>
					</div><!-- // sort-container -->
				</section>
